{
  "name": "beechit/default-upload-folder",
  "type": "typo3-cms-extension",
  "description": "Make it possible to configure the default upload folder for a certain TCA column",
  "license": [
    "GPL-2.0+"
  ],
  "require": {
    "typo3/cms-core": "^12.4"
  },
  "autoload": {
    "psr-4": {
      "BeechIt\\DefaultUploadFolder\\": "Classes/"
    }
  },
  "extra": {
    "typo3/cms": {
      "extension-key": "default_upload_folder"
    }
  }
}
